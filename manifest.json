{
  "manifest_version": 2,

  "name": "FB Autopoker",
  "description": "This extension autopokes people on Facebook.",
  "version": "1.0",

  "content_scripts": [{
    "matches": ["*://*.facebook.com/*"],
    "js": ["jquery-1.11.0.min.js", "fb-autopoker.js"],
    "run_at": "document_end"
  }],
  "offline_enabled": false
}
